{
  "name": "tfm-plataforma",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:shell": "npm --workspace app-shell run dev",
    "dev:registro": "npm --workspace registro run dev",
    "dev:all": "npm-run-all -p dev:registro dev:shell",
    "build": "npm --workspace registro run build && npm --workspace app-shell run build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tslib": "^2.8.1"
  }
}
